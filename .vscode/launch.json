  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Process",
      "port":3000
    },
    {
      "type":"node",
      "request": "launch",
      "name":"Debugger",
      "program":"cluster.js",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Test Debugger",
      "program": ".vscode/mocha.js",
      "envFile": "${workspaceFolder}/.env",
      "args":[
        "${workspaceFolder}/server/tests/routes/team-routes.delete.test.js"
      ]
    }
  ]
}
